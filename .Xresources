Xft.dpi: FIXED_DPI
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
! -----------------------------------------
! Colors
! -----------------------------------------
*.foreground:   #c5c8c6
*.background:   #1d1f21
*.cursorColor:  #c5c8c6
! black
*.color0:       #282a2e
*.color8:       #50565F
! red
*.color1:       #a54242
*.color9:       #DA4F4F
! green
! *.color2:       #8c9440
*.color2:       #336f75
*.color10:      #336f75
! yellow
*.color3:       #de935f
*.color11:      #f0c674
! blue
*.color4:       #5f819d
*.color12:      #81a2be
! magenta
*.color5:       #85678f
*.color13:      #b294bb
! cyan
