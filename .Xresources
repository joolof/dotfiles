#define termbg #1d1f21
#define termfg #c5c8c6
!-------------------------------------------------------------------------------
! ROFI Color theme - Alley
!-------------------------------------------------------------------------------
rofi.font: Iosevka Term Semibold 22
rofi.color-enabled: true
rofi.fullscreen: false
rofi.color-window: #111111, #111111, #111111
rofi.color-normal: #111111, #e8e8e8, #111111, #111111, #5f819d
rofi.color-active: #111111, #aaaaaa, #363b36, #111111, #151615
rofi.color-urgent: #fdf6e3, #dc322f, #eee8d5, #dc322f, #fdf6e3

!-------------------------------------------------------------------------------
! URxvt
!-------------------------------------------------------------------------------
!! Appearance
URxvt.termName: rxvt-unicode
URxvt.scrollBar: false
URxvt.foreground: termfg
URxvt*depth: 32
URxvt.background: [95]termbg
URxvt*internalBorder: 30

!! Font preferences
URxvt.font: FIXED_FONT
URxvt.boldFont: BOLD_FIXED_FONT
!URxvt.font: xft:Iosevka Term:pixelsize=30:weight=Semibold 
!URxvt.boldFont: xft:Iosevka Term:pixelsize=30:weight=Semibold 

!! Larger history limit
URxvt.saveLines: 5000

!! Perl extensions
URxvt.perl-ext-common: default,matcher,font-size 
!! Open urls in browser with Control-Click
URxvt.urlLauncher: /usr/bin/google-chrome
URxvt.matcher.button: C1
!! Change font size on Control-Plus/Minus
URxvt.keysym.Control-0x2b: perl:font-size:increase
URxvt.keysym.Control-0x2d: perl:font-size:decrease

URxvt.dynamicColors: on

